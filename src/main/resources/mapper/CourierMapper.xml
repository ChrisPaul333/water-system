<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.cjl.watersystem.mapper.CourierMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.cjl.watersystem.entity.Courier">
        <id column="courier_id" property="courierId" />
        <result column="courier_name" property="courierName" />
        <result column="counts" property="counts" />
        <result column="state" property="state" />
        <result column="phone" property="phone" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        courier_id, courier_name, counts, state, phone
    </sql>

</mapper>
