<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.cjl.watersystem.mapper.ShopMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.cjl.watersystem.entity.Shop">
        <id column="shop_id" property="shopId" />
        <result column="shop_name" property="shopName" />
        <result column="water_reserve" property="waterReserve" />
        <result column="dispenser_reserve" property="dispenserReserve" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        shop_id, shop_name, water_reserve, dispenser_reserve
    </sql>

</mapper>
